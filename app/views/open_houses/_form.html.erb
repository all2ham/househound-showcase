<%= simple_form_for [@open_house.listing, @open_house], wrapper: :prefix_input do |f| %>

  <div class="form-inputs">
    <%#= f.input :date, as: :string, input_html: { class: 'date_picker' } %>
    <%= f.input :start, as: :string, input_html: { class: 'date_time_picker' } %>
    <%= f.input :end, as: :string, input_html: { class: 'date_time_picker' } %>
  </div>

  <div class="form-actions">
    <%= link_to 'Back', listing_open_houses_path, class: 'button' %>
    <%= f.button :submit, class: 'right' %>
  </div>

<% end %>
